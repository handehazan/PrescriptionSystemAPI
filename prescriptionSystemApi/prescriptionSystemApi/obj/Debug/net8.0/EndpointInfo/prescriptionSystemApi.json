{
  "openapi": "3.0.1",
  "info": {
    "title": "prescriptionSystemApi",
    "version": "1.0"
  },
  "paths": {
    "/api/Auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Medicine": {
      "get": {
        "tags": [
          "Medicine"
        ],
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 1
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 10
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Medicine/SearchMedicine": {
      "get": {
        "tags": [
          "Medicine"
        ],
        "parameters": [
          {
            "name": "prefix",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Medicine/dowload-excel": {
      "get": {
        "tags": [
          "Medicine"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Medicine/test-parse": {
      "get": {
        "tags": [
          "Medicine"
        ],
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 1
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 4
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Medicine/refresh": {
      "post": {
        "tags": [
          "Medicine"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Prescription/CreatePrescription": {
      "post": {
        "tags": [
          "Prescription"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreatePrescriptionDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CreatePrescriptionDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CreatePrescriptionDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Prescription/{patientTC}": {
      "get": {
        "tags": [
          "Prescription"
        ],
        "parameters": [
          {
            "name": "patientTC",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Prescription/medicines/{prescriptionId}": {
      "get": {
        "tags": [
          "Prescription"
        ],
        "parameters": [
          {
            "name": "prescriptionId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Prescription/submit": {
      "post": {
        "tags": [
          "Prescription"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SubmitPrescriptionDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SubmitPrescriptionDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SubmitPrescriptionDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CreatePrescriptionDto": {
        "type": "object",
        "properties": {
          "patientTC": {
            "type": "string",
            "nullable": true
          },
          "visitDate": {
            "type": "string",
            "format": "date-time"
          },
          "doctorId": {
            "type": "string",
            "nullable": true
          },
          "medicines": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CreatePrescriptionMedicineDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreatePrescriptionMedicineDto": {
        "type": "object",
        "properties": {
          "medicineName": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SubmitPrescriptionDto": {
        "type": "object",
        "properties": {
          "prescriptionId": {
            "type": "integer",
            "format": "int32"
          },
          "pharmacyName": {
            "type": "string",
            "nullable": true
          },
          "medicinesGiven": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}